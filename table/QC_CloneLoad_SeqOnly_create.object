#!/bin/csh -f

#
# This table is used to create the Seq-Only QC report for the
# Clone Load Product (cloneload).
#
# It contains data produced by the last run of the clone loader.
#

#
# History
#
# 08/05/2003	dbm
#	- TR 3404 (JSAM)
#

cd `dirname $0` && source ./Configuration

cat - <<EOSQL | ${MGI_DBUTILS}/bin/doisql.csh ${DBSERVER} ${DBNAME} $0

use ${DBNAME}
go

create table QC_CloneLoad_SeqOnly (
	_Clone_key                     int             not null,
	mgiID                          varchar(30)     not null,
	seqID                          varchar(30)     not null,
	_JobStream_key                 int             not null,
	creation_date                  datetime        not null
)
go

checkpoint
go

quit

EOSQL
