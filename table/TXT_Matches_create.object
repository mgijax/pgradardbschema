#!/bin/csh -f

#
# Text Indexing - Matches
#
# A record in this table represents 
# a matching item for a given search.
#
#
#
# History
#
# 03/08/2005	lec
#	- TR 5915, MPR
#
#

cd `dirname $0` && source ./Configuration

cat - <<EOSQL | ${MGI_DBUTILS}/bin/doisql.csh ${DBSERVER} ${DBNAME} $0

use ${DBNAME}
go

create table TXT_Matches (
	_Search_key                    int             not null,
	_Object_key                    int             not null
)
go

checkpoint
go

quit

EOSQL
