#!/bin/csh -f

#
# This table is a representation of:
#	/data/downloads/clone_sets/RPCI/clonerpt_Mm.out.09_25_02
#
# A record in this table represents one clone id/seq id association.
#

#
# History
#
# 11/13/2002	lec
#	- TR 3404 (JSAM)
#
# 08/07/2003    dbm
#       - Added _JobStream_key
#       - Removed createdBy, modifiedBy and modification_date
#
# 10/15/2003    dbm
#       - Added _RPCIClone_key as the primary key
#
# 10/17/2003    dbm
#       - Changed clone_date (timestamp without time zone) to cloneDate (varchar)
#
# 12/29/2004    dbm
#       - Removed fpBin
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table radar.DP_RPCI_Clones (
	_RPCIClone_key                 int             not null,
	cloneID                        varchar(40)     not null,
	ends                           char(3)         null,
	fpSize                         int             null,
	chromosome                     char(4)         not null,
	status                         varchar(15)     not null,
	gCenter                        varchar(7)      null,
	cloneDate                      varchar(10)     not null,
	seqID                          varchar(30)     null,
	_JobStream_key                 int             not null,
	creation_date                  timestamp without time zone        not null
);
EOSQL
