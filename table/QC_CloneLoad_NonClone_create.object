#!/bin/sh

#
# This table is used to create the Non-Clone QC report for the
# Clone Load Product (cloneload).
#
# It contains data produced by the last run of the clone loader.
#

#
# History
#
# 08/05/2003	dbm
#	- TR 3404 (JSAM)
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table radar.QC_CloneLoad_NonClone (
	_Clone_key                     int             not null,
	mgiID                          varchar(30)     not null,
	_MGIType_key                   int             null,
	_JobStream_key                 int             not null,
	creation_date                  timestamp DEFAULT now()        not null
);
EOSQL
