#!/bin/sh

#
# This table is the MGI Format for the Clone Load Product (cloneload).
#
# It contains clone/accession ID association.
#

#
# History
#
# 11/13/2002	lec
#	- TR 3404 (JSAM)
#
# 07/03/2003	dbm
#	- Added private column
#
# 08/07/2003	dbm
#	- Added _JobStream_key
#	- Removed createdBy, modifiedBy and modification_date
#
# 10/14/2003	dbm
#	- Added _CloneAccession_key
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table radar.MGI_CloneLoad_Accession (
	_CloneAccession_key            int             not null,
	_Clone_key                     int             not null,
	accID                          varchar(30)     not null,
	logicalDB                      varchar(80)     not null,
	private                        smallint             not null,
	_JobStream_key                 int             not null,
	creation_date                  timestamp DEFAULT now()        not null
);
EOSQL
