#!/bin/csh -f

#
# This table is the NIA parent-daughter clone mapping.
#
# It contains parent-daughter clone ID pairs for NIA.
#

#
# History
#
# 07/14/2003	dbm
#	- TR 3404 (JSAM)
#

cd `dirname $0` && source ./Configuration

cat - <<EOSQL | ${MGI_DBUTILS}/bin/doisql.csh ${DBSERVER} ${DBNAME} $0

use ${DBNAME}
go

create table NIA_Parent_Daughter_Clones (
	parentCloneID                  varchar(30)     not null,
	daughterCloneID                varchar(30)     not null,
	origin                         varchar(5)      not null,
	cloneLibrary                   varchar(255)    not null
)
go

checkpoint
go

quit

EOSQL
