#!/bin/csh -f

#
# This table is the Application Job Stream.
#
# A record in this table represents the logging of the execution
#
# History
#
# 08/05/2003	lec
#	- TR 3404 (JSAM)
#
#

cd `dirname $0` && source ./Configuration

cat - <<EOSQL | ${MGI_DBUTILS}/bin/doisql.csh ${DBSERVER} ${DBNAME} $0

use ${DBNAME}
go

create table APP_JobStream (
	_JobStream_key                 int             not null,
	jobStreamName                  varchar(30)     not null,
	createdBy                      varchar(30)     not null,
	returnCode                     int null,
	start_date                     datetime        not null,
	end_date                       datetime null
)
go

checkpoint
go

quit

EOSQL
