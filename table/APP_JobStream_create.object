cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

# A record in this table represents the logging of the execution
#
# History
#
# 08/05/2003	lec
#	- TR 3404 (JSAM)
#
#

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${MGI_DBUTILS}/bin/doisql.csh ${DBSERVER} ${DBNAME} $0

create table radar.APP_JobStream (
EOSQL
	_JobStream_key                 int             not null,
	jobStreamName                  varchar(30)     not null,
	createdBy                      varchar(30)     not null,
	returnCode                     int null,
	start_date                     timestamp DEFAULT now()        not null,
	end_date                       timestamp DEFAULT now() null
);
go

checkpoint
go

quit

EOSQL
