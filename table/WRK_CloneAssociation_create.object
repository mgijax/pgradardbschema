#!/bin/sh

#
# This table is the clone mapping for the Clone Load Product (cloneload).
#
# It contains mappings between clones in the RADAR and MGD databases.
#

#
# History
#
# 06/27/2003	dbm
#	- TR 3404 (JSAM)
#
# 07/08/2003	dbm
#	- Removed segmentType and vectorType
#
# 11/26/2003	dbm
#	- Renamed from WRK_CloneMapping to WRK_CloneAssociation
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table radar.WRK_CloneAssociation (
	_Clone_key                     int             not null,
	accID                          varchar(30)     not null,
	logicalDB                      varchar(80)     not null,
	private                        smallint             not null,
	_Object_key                    int             null,
	_MGIType_key                   int             null,
	mgiID                          varchar(30)     null,
	_Source_key                    int             null,
	_Refs_key                      int             null
);
EOSQL
