#!/bin/sh

#
# This table is a representation the standard IMSR Data load format.  Data
# is placed here prior to be loaded to the IMSR database.
#
# A record in this table represents a strain from a provider site and one set
# of associated genetic information (chrom, gene, allele, mutation type).
# An additional record will exist for each gene/allele combination associated
# with the strain.
#

#
# History
#
# 07/19/2004	dow
#	- Initial IMSR release
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table radar.MGI_IMSR (
	    stock_ID                        varchar(30)     not null,
            stock_designation               varchar(255)    not null,
	    strain_type                     varchar(255)    null,
            state                           varchar(50)     not null,
	    mutation_type                   varchar(255)    null,
	    chromosome                      varchar(20)     null,
            gene_accID                      varchar(50)     null,
            gene_symbol                     varchar(255)    null,
            gene_name                       varchar(255)    null,
            allele_accID                    varchar(50)     null,
            allele_symbol                   varchar(255)    null,
            allele_name                     varchar(255)    null,
            site                            varchar(20)     null,
            url                             varchar(255)    null,
	    file_name                       varchar(255)    null,
	    line_number                     integer         null,
	    creation_date                   timestamp without time zone        null,
            modification_date               timestamp without time zone        not null
);
EOSQL
