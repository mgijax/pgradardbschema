#!/bin/csh -f

#
# This table is the MGI ID to IMAGE ID mapping.
#
# It contains MGI ID and IMAGE ID pairs.
#

#
# History
#
# 07/14/2003	dbm
#	- TR 3404 (JSAM)
#

cd `dirname $0` && source ./Configuration

cat - <<EOSQL | ${MGI_DBUTILS}/bin/doisql.csh ${DBSERVER} ${DBNAME} $0

use ${DBNAME}
go

create table IMG_MGI_IMAGE (
	mgiID                          varchar(30)     not null,
	imageID                        varchar(30)     not null
)
go

checkpoint
go

quit

EOSQL
