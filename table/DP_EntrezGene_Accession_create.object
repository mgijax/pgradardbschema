cd `dirname $0` && source ./Configuration

cat - <<EOSQL | ${MGI_DBUTILS}/bin/doisql.csh ${DBSERVER} ${DBNAME} $0

use ${DBNAME}
go

create table DP_EntrezGene_Accession (
	taxID      	integer 	not null,
	geneID          varchar(30)     not null,
	status          varchar(15)     null,
	rna             varchar(30)     null,
	rnagi           varchar(30)     not null,
	protein         varchar(30)     not null,
	proteingi       varchar(30)     not null,
	genomic         varchar(30)     not null,
	genomicgi       varchar(30)     not null,
	startPos        integer         not null,
	endPos          integer         not null,
	orientation     char(1)         not null,
	assembly	varchar(100)	not null
)
go

checkpoint
go

quit

EOSQL
