#!/bin/sh

#
# History
#
# 01/28/20093	marka
# - adding indexes to improve imsrload performance for imsrload-4-2-0-1
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create index WRK_IMSR_Alleles_idx_wrkimsralleles_accid on radar.WRK_IMSR_Alleles(accID);


create index WRK_IMSR_Alleles_idx_wrkimsralleles_symbol on radar.WRK_IMSR_Alleles(symbol);


create index WRK_IMSR_Alleles_idx_wrkimsralleles_tmpkey on radar.WRK_IMSR_Alleles(tmpAlleleKey);


EOSQL
