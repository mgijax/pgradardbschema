#!/bin/sh

#
# History
#
# 01/28/20093	marka
# - adding indexes to improve imsrload performance for imsrload-4-2-0-1
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create index WRK_IMSR_Strains_idx_wrkimsrstrains_mgistrkey on radar.WRK_IMSR_Strains(MGI_Strain_key)
; 

create index WRK_IMSR_Strains_idx_wrkimsrstrains_stockid on radar.WRK_IMSR_Strains(stock_ID)
;

create index WRK_IMSR_Strains_idx_wrkimsrstrains_state on radar.WRK_IMSR_Strains(strain_state)
;

create index WRK_IMSR_Strains_idx_wrkimsrstrains_des on radar.WRK_IMSR_Strains(stock_designation)
;


EOSQL
